<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.anipick.backend.test.TestMapper">
    <select id="findAllUsers" resultType="com.anipick.backend.test.TestAllUsersDto">
        SELECT u.user_id AS userId,
               u.email AS email
        FROM User u
    </select>
</mapper>